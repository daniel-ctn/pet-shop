onst orderItem: CartModel = {\r\n                    puppy: data,\r\n                    qty: item.qty\r\n                }\r\n                orderItems.push(orderItem)\r\n            }))\r\n\r\n            const order: FullOrderModel = {\r\n                _id: data._id,\r\n                orderItems,\r\n                paymentMethod: data.paymentMethod,\r\n                shippingAddress: data.shippingAddress,\r\n                prices: {\r\n                    itemsPrice: data.itemsPrice,\r\n                    shippingPrice: data.shippingPrice,\r\n                    taxPrice: data.taxPrice,\r\n                    totalPrice: data.totalPrice\r\n                },\r\n                isPaid: data.isPaid,\r\n                isDelivered: data.isDelivered,\r\n                user: data.user\r\n            }\r\n\r\n            dispatch({\r\n                type: CartActionTypes.GET_ORDER,\r\n                payload: order\r\n            })\r\n        } catch (e) {\r\n            console.log(e)\r\n        }\r\n    }\r\n}\r\n\r\nexport const payOrder = function (paymentResult: PaymentResultModel, id: string, token: string): ThunkAction<Promise<void>, {}, {}, AnyAction> {\r\n    return async (dispatch: ThunkDispatch<{}, {}, AnyAction>): Promise<void> => {\r\n        try {\r\n            const {data} = await axios.put(`/api/order/${id}/pay`, paymentResult,\r\n                {\r\n                    headers: {\r\n                        'Content-Type': 'application/json',\r\n                        Authorization: `Bearer ${token}`\r\n                    }\r\n                })\r\n\r\n            dispatch({\r\n                type: CartActionTypes.ORDER_PAY\r\n            })\r\n        } catch (e) {\r\n            console.log(e)\r\n        }\r\n    }\r\n}\r\n","C:\\Development\\Projects\\pet-shop\\frontend\\src\\state\\reducers\\userReducers.ts",[],"C:\\Development\\Projects\\pet-shop\\frontend\\src\\pages\\LoginPage.tsx",[],"C:\\Development\\Projects\\pet-shop\\frontend\